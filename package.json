{
    "name": "bmc-tpl",
    "displayName": "BMC-tpl",
    "description": "BMC TPL (language) Support",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.63.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
    "contributes": {
        "languages": [
            {
                "id": "tpl",
                "aliases": [
                    "tpl"
                ],
                "extensions": [
                    ".tpl"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "tplpre",
                "aliases": [
                    "tplpre"
                ],
                "extensions": [
                    ".tplpre"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "tpl",
                "scopeName": "source.tpl",
                "path": "./syntaxes/tpl.tmLanguage.json"
            },
            {
                "language": "tplpre",
                "scopeName": "source.tplpre",
                "path": "./syntaxes/tpl.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    }
}
